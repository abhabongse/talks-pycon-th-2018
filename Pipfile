verify_ssl = true
name = "pypi"
url = "https://pypi.org/simple"

[requires]
python_version = "3.6"

[scripts]
post-install = "bash -c 'jupyter-nbextension install rise --py --sys-prefix && jupyter-nbextension enable rise --py --sys-prefix && jupyter nbextensions_configurator enable --sys-prefix'"

[packages]
jupyter = ">=1.0.0"
rise = ">=5.2.0"
jupyter-nbextensions-configurator = ">=0.4.0"

[dev-packages]
